{
  "name": "Ensemble Learning 256 with aux",
  "seed": 7171,
  "data": {
    "dirname": "data/orig",
    "val_size": 0.2,
    "batch_size": 1,
    "volume_size": [256, 256, 64],
    "seq_type": "FLAIR",
    "augmentations": [
      {
        "rotate": {
          "x_limit": [-15, 15],
          "y_limit": [0, 0],
          "z_limit": [0, 0],
          "p": 0.5 
        }
      },
      {
        "elastic_transform": {
          "deformation_limits": [0, 0.25],
          "interpolation": 2,
          "p": 0.1
        }
      },
      {
        "flip": {
          "axis": 0,
          "p": 0.5
        }
      },
      {
        "flip": {
          "axis": 1,
          "p": 0.5
        }
      },
      {
        "flip": {
          "axis": 2,
          "p": 0.5
        }
      },
      {
        "random_rotate_90": {
          "axes": [1, 2],
          "p": 0.5
        }
      },
      {
        "gaussian_noise": {
          "var_limit": [0, 5],
          "p": 0.2
        }
      },
      {
        "random_gamma": {
          "gamma_limit": [0.5, 1.5],
          "p": 0.2
        }
      }
    ]
  },
  "model": {
    "name": "baseline",
    "use_aux": false
  },
  "train": {
    "lr": 1e-4,
    "decay_steps": 1000,
    "decay_rate": 0.96,
    "epoch": 100
  },
  "desc": "Ensemble learning experiment with 256x256x64 voxel size, with aux out."
}
